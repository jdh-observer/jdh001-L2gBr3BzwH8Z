@phdthesis{perkins_where_2012,
	address = {Dublin},
	type = {M.{Phil} {Dissertation}},
	title = {"{Where} and what is {Ireland}?" {The} {Capuchin} {Annual} and the editorship of {Fr}. {Senan} {Moynihan}, 1930-1954},
	shorttitle = {"{Where} and what is {Ireland}?"},
	abstract = {The Capuchin Annual was published by the Irish Province of the Capuchin
Franciscans from 1930–77, but this dissertation concerns itself with the period of Fr. Senan Moynihan’s editorship, 1930–54. The objective of this dissertation is to offer an historical assessment of The Capuchin Annual and to contextualize it within the periodical culture of the time. 
The foundation of this study is The Capuchin Annual and each edition published 1930–54 has been surveyed. Selected contemporaneous issues of The Dublin Magazine, Ireland To-Day, The Bell, Envoy, Bonaventura, Fr.  Matthew Record, The Catholic Bulletin and Irish Bookman have been examined to offer comparative analysis.
The uncatalogued papers and correspondence relating to The Capuchin Annual held at The Irish Capuchin Provincial Archives, Dublin, has provided much of the primary material for this study. The correspondence and papers of Fr. Senan held in the Thomas McGreevy Archive at Trinity College, Dublin; Leeds University Library Brotherton Special Collections; and in the Roman Catholic Archdiocese of Perth Archives have also yielded valuable material.
Advertisements and contemporary reviews from the Irish Times, Irish Press and Irish Independent have been sourced through the Irish Newspaper Archive (http://www.irishnewsarchive.com).
This dissertation argues that there was a seismic shift in the editorial mission of The Capcuhin Annual during Fr. Senan’s editorship that studies to date have failed to interrogate. Though the Franciscan ethos endured, within a few years the cultural agenda dominated, positioning The Capuchin Annual at the centre of national cultural life and, furthermore, projecting a version of Ireland and Irish identity to 25,000 readers in Ireland and internationally. The Capuchin Annual played a significant role in defining the culture of Ireland in the turbulent early decades of independent Ireland.},
	language = {Anglais},
	school = {Trinity College},
	author = {Perkins, Sonya},
	year = {2012},
	file = {Perkins - 2012 - Where and what is Ireland The Capuchin Annual a.pdf:C\:\\Users\\maell\\Zotero\\storage\\F4MPQTK2\\Perkins - 2012 - Where and what is Ireland The Capuchin Annual a.pdf:application/pdf}
}

